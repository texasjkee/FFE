            <li class="author__list-item ">
              <a href="http://localhost:3333/article">Add article</a>
            </li>
            <li class="author__list-item ">
              <a href="http://localhost:3333/viewArticle">View Article</a>
            </li>
          </ul>
        </nav>
      </div>
    </div>
  </div>
  <script src="/js/author.js"></script>
