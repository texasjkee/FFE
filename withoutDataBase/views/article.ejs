<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles/article.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.3.6/axios.min.js"
    integrity="sha512-06NZg89vaTNvnFgFTqi/dJKFadQ6FIglD6Yg1HHWAUtVFFoXli9BZL4q4EO1UTKpOfCfW5ws2Z6gw49Swsilsg=="
    crossorigin="anonymous" referrerpolicy="no-referrer"></script>
  <title>Document</title>
</head>

<body>
  <div class="content">
    <div class="posts">
      <div class="posts-wrapper">
        <form id="arcticle-form">
          <div class="form-info">
            <label>
              <input class="post-title" type="text" name="title" placeholder="Name article" >
            </label>
          </div>
          <div class="form-text">
            <label>
              <textarea class="post-body" name="text" cols="40" rows="5" placeholder="Text article" ></textarea>
            </label>
          </div>
          <input type="submit" class="save-article__button"  value="Save">
          <input type="text" class="search-author">
          <div class="invisibleDiv"></div>
          <!-- <div class="form-button">
          </div> -->
        </form>
        <ul>
          <li><a href="http://localhost:3333/author">Author</a></li>
          <li><a href="http://localhost:3333/article">Article</a></li>
          <li><a href="http://localhost:3333/viewArticle">ViewArticle</a></li>
        </ul>
      </div>
    </div>
  </div>
  <script type="module" src="/js/article.js"></script>
</body>
</html>